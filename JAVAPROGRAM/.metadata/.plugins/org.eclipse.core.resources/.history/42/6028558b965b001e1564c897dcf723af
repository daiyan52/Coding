package TCS.in;
public class CountFreq {
	public static void main(String[] args) {
		 int arr[] = {0,2,1,1,2,2,6,1};
		 int temp[] = new int[100];
		 int k=0;
		 int count=0;
		 for(int i=0;i<arr.length;i++) {
			 if(arr[i]==arr[i+1]) {
				 temp[arr[i]-(char)0]++;
			 }
		 }
		 for(int i=0;i<temp.length;i++) {
			 System.out.print(temp[i]+"  ");
		 }
	}
}
 